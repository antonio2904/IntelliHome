<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.antony.project">

    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViews"
            android:screenOrientation="portrait"/>
        <activity android:name=".Control"
            android:screenOrientation="portrait"/>
        <activity android:name=".Signuporin"
            android:screenOrientation="portrait"/>
        <activity android:name=".SignUPActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".Analyse"
            android:screenOrientation="portrait"/>
        <activity android:name=".Set"
            android:screenOrientation="portrait"/>
        <activity android:name=".Bluetooth"
            android:screenOrientation="portrait"/>
    </application>

</manifest>